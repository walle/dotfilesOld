# cd
alias ..='cd ..'
 
#nano
alias nano='nano -w'

# ls
alias ls="ls -A --color=auto"
alias l="ls -lAh --color=auto"
alias ll="ls -l --color=auto"
alias la='ls -la --color=auto'
 
